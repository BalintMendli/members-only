<h2>Sign In</h2>
<%= form_for :session do |f|%>
<%= f.label :email, 'Email: ' %>
<%= f.email_field :email%><br>
<%= f.label :password, 'Password: ' %>
<%= f.password_field :password%><br>
<div class="remember">
  <%= label_tag :remember_me, 'Remember Me:' %>
  <%= check_box_tag :remember_me %>
</div>
<%= f.submit 'Sign in' %>
<%end %>